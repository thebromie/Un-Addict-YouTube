{
  "description": "Blocks YouTube Videos Based on Number of Videos Watched",
  "manifest_version": 2,
  "name": "Un-Addict YouTube",
  "version": "0.0.0",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/apply-css",

  "background": {
    "scripts": ["background.js"]
  },

  "page_action": {
    "default_icon": "icons/off.svg",
    "browser_style": true
  },
  
  "permissions": [
    "activeTab",
    "tabs"
  ]
}
